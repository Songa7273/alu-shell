#!/usr/bin/env bash

# Initialize the hour and minute counters
hour=0

# Loop through hours from 0 to 12
while [ $hour -le 12 ]; do
    echo "Hour: $hour"
    
    # Initialize minute counter
    minute=1
    
    # Loop through minutes from 1 to 59
    while [ $minute -le 59 ]; do
        echo "$minute"
        minute=$((minute + 1))
    done
    
    # Increment the hour
    hour=$((hour + 1))
done

